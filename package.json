{
  "name": "fw-honeypot",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "dev": "DEBUG='*' nodemon server.js",
    "format:write": "npx -y prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "start": "DEBUG='*' node server.js",
    "test": "node --test",
    "docker:build": "docker build -t fw-honeypot .",
    "registry:login": "echo $GITHUB_TOKEN | docker login ghcr.io -u neophob --password-stdin",
    "docker:tag": "docker tag fw-honeypot:latest ghcr.io/neophob/fw-honeypot:latest",
    "docker:push": "docker push ghcr.io/neophob/fw-honeypot:latest",
    "docker:run": "docker run --rm  -p 445:445 -p 3306:3306 -p 3322:3322 -p 3323:3323 -p 3325:3325 -p 8080:8080 fw-honeypot",
    "build": "npm run docker:build && npm run docker:push && npm run docker:tag"
  },
  "description": "A rudimentary and extensible honeypot framework for managing blacklists, IP blocking, and integration with firewalls. (Beta)",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:22/SomethingWithPhp/fw-honeypot.git"
  },
  "bugs": {
    "url": "https://github.com/SomethingWithPhp/fw-honeypot/issues"
  },
  "homepage": "https://github.com/SomethingWithPhp/fw-honeypot",
  "type": "module",
  "author": "Pascal Schnell<schnell.pascale@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "cidr-tools": "^11.0.3",
    "debug": "^4.4.3",
    "ip-regex": "^5.0.0",
    "smtp-server": "^3.16.1"
  }
}
